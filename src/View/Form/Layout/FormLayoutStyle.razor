@namespace Skclusive.Mobx.Form
@inherits StyleComponentBase

.FormLayout-Root {
    display: flex;
    flex: 1 1;
    margin: 0px;
}

.FormLayout-Fluid {
    flex: '';
}

.FormLayout-Center {
    align-items: center;
    display: flex;
    height: 100%;
    justify-content: center;
    padding-left: 5px;
    padding-right: 5px;
}

.FormLayout-Row {
    flex-direction: row;
}

.FormLayout-Column {
    flex-direction: column;
}

