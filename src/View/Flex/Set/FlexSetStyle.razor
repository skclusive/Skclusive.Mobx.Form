@namespace Skclusive.Mobx.Form
@inherits StyleComponentBase

.FlexSet-Root {
    display: flex;
    flex: 1 1;
    margin: 0px;
}

.FlexSet-Fluid {
    flex: '';
}

.FlexSet-Center {
    align-items: center;
    display: flex;
    height: 100%;
    justify-content: center;
    padding-left: 5px;
    padding-right: 5px;
}

.FlexSet-Row {
    flex-direction: row;
}

.FlexSet-Column {
    flex-direction: column;
}

